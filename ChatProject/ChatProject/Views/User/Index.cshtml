
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-hover">
 
    @foreach (var item in ViewBag.Users)
    {
        <tr>

            <td>@item.Firstname</td>
            <td>@item.Surname</td>
            <td>@item.Username</td>
            
        </tr>
       
    }


</table>   